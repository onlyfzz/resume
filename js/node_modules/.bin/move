#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  --stack_trace_limit=25 "$basedir/../move/bin/move" "$@"
  ret=$?
else 
  node  --stack_trace_limit=25 "$basedir/../move/bin/move" "$@"
  ret=$?
fi
exit $ret
